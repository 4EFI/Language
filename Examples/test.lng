варибля Петрович ( варибля а , варибля б )
!:
    принтф ( а + б ) 
    а + б ;
:!

!% --------------------------------------------------------------- %!

варибля Мейн ( )
!:
    !%
    варибля а = 0 
    варибля б = 0
    варибля ц = 0

    сканф ( а , б , ц )

    %!

    варибля и = Петрович ( 2 , 10 )
    принтф ( и )
    живем   и < 5
    !:
        варибля  локал = 11
        принтф ( локал )

        проверочка и == 0 
        !:
            варибля  опа = 100
            принтф ( кор ( опа ) )
        :!

        и = и + 1      
    :!
:!

